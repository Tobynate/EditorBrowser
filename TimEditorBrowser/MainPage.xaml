<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TimEditorBrowser.MainPage">

    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Navigate" x:Name="NavigateButton" HeightRequest="0" Clicked="NavigateButton_Clicked"
                    BackgroundColor="DarkKhaki" TextColor="MidnightBlue"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
